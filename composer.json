        "source": "https://github.com/portrino/px_shopware"
    },
    "require": {
        "ext-curl": "*",
        "lib-curl": "^7",
        "php": ">=7.2.0",

        "typo3/cms-core": "^9.5 || ^10.4"
    },
    "autoload": {
        "psr-4": {
            "Portrino\\PxShopware\\": "Classes"
        }
    },
    "replace": {
        "portrino/px_shopware": "*",
        "typo3-ter/px_shopware": "*"
